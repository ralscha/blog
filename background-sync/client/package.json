{
  "name": "syncclient",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build",
    "dev": "npm-run-all --parallel sw-dev-webpack ionic-start",
    "ionic-start": "ionic serve",
    "ng": "ng",
    "postbuild": "npm run sw-prod-webpack",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "start": "ng serve -o",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "rimraf ./dist/app/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --color && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "19.2.1",
    "@angular/compiler": "19.2.1",
    "@angular/core": "19.2.1",
    "@angular/forms": "19.2.1",
    "@angular/platform-browser": "19.2.1",
    "@angular/platform-browser-dynamic": "19.2.1",
    "@angular/router": "19.2.1",
    "@ionic/angular": "8.4.3",
    "dexie": "4.0.11",
    "ionicons": "7.4.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "uuid": "11.1.0",
    "workbox-core": "7.3.0",
    "workbox-precaching": "7.3.0",
    "workbox-routing": "7.3.0",
    "workbox-strategies": "7.3.0",
    "workbox-window": "7.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.1",
    "@angular/cli": "19.2.1",
    "@angular/compiler-cli": "19.2.1",
    "@ionic/angular-toolkit": "12.1.1",
    "local-web-server": "5.4.0",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "ts-loader": "9.5.2",
    "typescript": "5.8.2",
    "webpack-cli": "5.1.4",
    "workbox-cli": "7.3.0"
  }
}