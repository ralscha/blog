{
  "name": "ngworkbox",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all --parallel sw-dev-webpack start",
    "start": "ng serve -o",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "postbuild-prod": "npm run sw-prod-webpack",
    "serve-dist": "ws --hostname localhost -d dist/ngworkbox -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "sw-dev-webpack": "rimraf ./src/service-worker.js && webpack --config ./sw-dev-webpack.config.js",
    "sw-prod-webpack": "rimraf ./dist/ngworkbox/service-worker.js && webpack --config ./sw-prod-webpack.config.js --progress --colors && workbox injectManifest ./workbox-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.3",
    "@angular/common": "9.1.3",
    "@angular/compiler": "9.1.3",
    "@angular/core": "9.1.3",
    "@angular/forms": "9.1.3",
    "@angular/platform-browser": "9.1.3",
    "@angular/platform-browser-dynamic": "9.1.3",
    "@angular/router": "9.1.3",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "workbox-core": "5.1.2",
    "workbox-precaching": "5.1.2",
    "workbox-routing": "5.1.2",
    "workbox-strategies": "5.1.2",
    "workbox-window": "5.1.2",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.3",
    "@angular/cli": "9.1.3",
    "@angular/compiler-cli": "9.1.3",
    "@angular/language-service": "9.1.3",
    "@types/node": "13.13.2",
    "codelyzer": "5.2.2",
    "local-web-server": "4.0.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "ts-loader": "7.0.1",
    "tslint": "6.1.1",
    "typescript": "3.8.3",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "workbox-cli": "5.1.2"
  }
}
