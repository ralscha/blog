{
  "name": "protobuf",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build",
    "ng": "ng",
    "pbts": "pbjs -t static-module --es6 -w es6 ../server/src/main/protobuf/Earthquake.proto -o src/app/protos/earthquake.js && pbts --no-comments src/app/protos/earthquake.js -o src/app/protos/earthquake.d.ts",
    "serve-dist": "ws --hostname localhost -d dist/app -p 1234 -o --log.format stats",
    "start": "ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.2.3",
    "@angular/compiler": "20.2.3",
    "@angular/core": "20.2.3",
    "@angular/forms": "20.2.3",
    "@angular/platform-browser": "20.2.3",
    "@angular/router": "20.2.3",
    "@ionic/angular": "8.7.3",
    "ionicons": "8.0.13",
    "protobufjs": "7.5.4",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.2.1",
    "@angular/cli": "20.2.1",
    "@angular/compiler-cli": "20.2.3",
    "@ionic/angular-toolkit": "12.3.0",
    "local-web-server": "5.4.0",
    "protobufjs-cli": "1.1.3",
    "typescript": "5.9.2"
  }
}
